pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
function _init()
	-- board position in px
	_board_x = 32
	_board_y = 32
	
 -- crosshair in pos
 _crosshair_x = 1
 _crosshair_y = 1
 _crosshair_counter  = 20
 _crosshair_waittime = 5

 -- hammer
 _hammer = 0
 
	-- lines of board
	_l1 = {0,0,0,0}
	_l2 = {0,0,0,0}
	_l3 = {0,0,0,0}
	_l4 = {0,0,0,0}
	_board = {_l1,_l2,_l3,_l4}
	
	-- push rabbits out!
	_p1 = {0,0,0,0}
	_p2 = {0,0,0,0}
	_p3 = {0,0,0,0}
	_p4 = {0,0,0,0}
	_push = {_p1,_p2,_p3,_p4}
	
	-- counter
	_counter = 10
	_counter_reset = 10
	
	-- points
	_points = 0
end

function _draw()
	rectfill(0,0,128,128,9)
	draw_board()	
	draw_crosshair(_crosshair_x,_crosshair_y)
	draw_hammer(_crosshair_x,_crosshair_y)
end

function _update()
 fire_hammer()
	move_crosshair()
	update_holes()
	
	if(_counter > 0) then
		_counter -= 1
	else
		fill_hole()
		_counter = _counter_reset
	end
end

-- number functions
function draw_points(x,y)
	p1 = 
end

function draw_number(number,x,y)
 spr(49+number,x,y)
end

-- helper
function set_target_hole(val)
	_push[_crosshair_x][_crosshair_y] = val
	_board[_crosshair_x][_crosshair_y] = val
end

function has_hit_rabbit()
 if(_push[_crosshair_x][_crosshair_y] > 0) then
  return true
 end
 if(_board[_crosshair_x][_crosshair_y] > 0) then
  return true
 end
end

-- hammer handling
function draw_hammer(x,y)
 if (_hammer > 0) then
 	hx = (x*16)+_board_x-16
 	hy = (y*16)+_board_y-16-_hammer
 	spr(010, hx    , hy)
 	spr(011, hx+8  , hy)
 	spr(026, hx    , hy+8)
 	spr(027, hx+8  , hy+8)
 	_hammer -= 1
	end
end

function fire_hammer()
 if(btn(4)) then
  _hammer = 5
  set_target_hole(0)
 end
end

-- crosshair handling
function move_crosshair()
 if(_crosshair_counter > 0) then
  _crosshair_counter -= 1
 else
 	if(btn(0)) then 
 	 if(_crosshair_x == 1) then return end
 		_crosshair_x -= 1
 		_crosshair_counter = _crosshair_waittime 
 	end
 	if(btn(1)) then 
 	 if(_crosshair_x == 4) then return end
 		_crosshair_x += 1
 		_crosshair_counter = _crosshair_waittime 
 	end
 	if(btn(2)) then 
 	 if(_crosshair_y == 1) then return end
 		_crosshair_y -= 1
 		_crosshair_counter = _crosshair_waittime 
 	end
 	if(btn(3)) then 
 	 if(_crosshair_y == 4) then return end
 		_crosshair_y += 1
 		_crosshair_counter = _crosshair_waittime 
 	end
	end
end

function draw_crosshair(x,y)
 hx = (x*16)+_board_x-16
 hy = (y*16)+_board_y-16
 spr(008, hx    , hy)
 spr(009, hx+8  , hy)
 spr(024, hx    , hy+8)
 spr(025, hx+8  , hy+8)
end

-- hole handling
function fill_hole()
	x = flr(rnd(4)+1)
	y = flr(rnd(4)+1)
	if(_push[x][y] == 0) then
		if(_board[x][y] == 0) then
			_push[x][y] = 100
		end
	else
		fill_hole()
	end
end

function update_holes()
	for x=1,4 do
		for y=1,4 do
			if(_push[x][y] == 0) then
				if(_board[x][y] > 0) then
			 	_board[x][y] = _board[x][y]-1
				end
			else
				_board[x][y] += 2
				_push[x][y] -= 2
			end
		end
	end
end

function hole(x,y)
	if (_board[x][y] > 0) then
		rabbit_x = x*16+_board_x-20
		rabbit_y = y*16+_board_y-(_board[x][y]/4)
		draw_rabbit(rabbit_x,rabbit_y)
	end
end

-- draw rabbit
function draw_rabbit(x,y)
	spr(065,x+8	,y)
	spr(080,x			,y+8)
	spr(081,x+8	,y+8)
	spr(082,x+16,y+8)
	spr(097,x+8	,y+16)
end

-- draw game board overlay
function draw_bottom_line(x,y)
	rabbit_hider(x,y)
	for i=0,3 do
		spr(021, i*16+x					,y)
		spr(022,(i*16)+8+x 	,y)
	end
end

function draw_top_line(x,y)
	rabbit_hider(x,y)
	for i=0,3 do

		spr(005, i*16+x					,y		)
		spr(006,(i*16)+8+x 	,y		)
	end
end

function draw_line(x,y)
	rabbit_hider(x,y)
	for i=0,3 do
		spr(005,(i*16)+x			,y+8)
		spr(006,(i*16)+8+x	,y+8)
		spr(021,(i*16)+x			,y)
		spr(022,(i*16)+8+x	,y)
	end
end

function rabbit_hider(x,y)
		rectfill(x,y+8,x+63,y+32,9)
end

-- draw the board
function draw_board()
draw_top_line(_board_x,_board_y)
	for i=1,4 do
		hole(i,1)
	end
	draw_line(_board_x,_board_y+8)
	for i=1,4 do
		hole(i,2)
	end
	draw_line(_board_x,_board_y+24)
	for i=1,4 do
		hole(i,3)
	end
	draw_line(_board_x,_board_y+40)
	for i=1,4 do
		hole(i,4)
	end
	draw_bottom_line(_board_x,_board_y+56)
end
